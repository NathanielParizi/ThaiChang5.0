<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/grandient_background3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/mystuffRecyclerView"
        android:layout_width="match_parent"
        android:layout_height="135dp"
        android:layout_below="@id/Status"
        android:layout_marginTop="197dp"
        android:orientation="horizontal" />

    <ImageView
        android:id="@+id/profile_image"

        android:layout_width="177dp"
        android:layout_height="167dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:fadingEdge="horizontal|vertical"
        android:fadingEdgeLength="100dp"
        android:scaleType="fitXY"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/Clear"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/Location"
        android:layout_marginTop="21dp"
        android:maxLength="20"
        android:maxLines="1"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:text="Name"
        android:textColor="@color/Black"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="184dp" />

    <TextView
        android:id="@+id/Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="80dp"
        android:maxLength="11"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="434dp"
        tools:text="Location" />

    <TextView
        android:id="@+id/Age"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Location"
        android:layout_below="@+id/Location"
        android:maxLength="8"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="265dp"
        tools:text="Age" />

    <TextView
        android:id="@+id/Gender"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Location"
        android:layout_below="@+id/Age"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="298dp"
        tools:text="Gender" />

    <TextView
        android:id="@+id/PhoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Location"
        android:layout_below="@+id/Gender"
        android:maxLength="10"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="332dp"
        tools:text="Phone #" />

    <TextView
        android:id="@+id/inventory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/Location"
        android:layout_marginBottom="137dp"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:text="What I Have:"
        android:textColor="@color/Black"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/Status"
        android:layout_width="319dp"
        android:layout_height="wrap_content"

        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/Location"
        android:layout_marginTop="180dp"
        android:maxLength="200"
        android:maxLines="3"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:textColor="@color/Black"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/Interests"
        android:layout_width="319dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/Location"
        android:layout_marginBottom="166dp"
        android:maxLines="9"
        android:shadowColor="@color/White"
        android:shadowRadius="15"
        android:text="String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();String current_user_id = mCurrentUser.getUid();"
        android:textColor="@color/Black"
        android:textSize="14sp" />

</RelativeLayout>